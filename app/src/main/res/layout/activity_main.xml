    <dji.ux.beta.core.panel.topbar.TopBarPanelWidget
        android:id="@+id/panel_top_bar"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="@dimen/top_bar_percent_height"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/TopBarPanelStyle"
        />

    <!-- アプリカスタムビュー -->
    <TextView
        android:id="@+id/txt_product"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/black"
        android:textColor="@color/white"
        android:paddingStart="@dimen/txt_product_name_margin"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="@string/txt_product_default"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/txt_status_message"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/black"
        android:textColor="@color/white"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:text="(status)"
        />

    <dji.ux.panel.CameraSettingExposurePanel
        android:id="@+id/camera_setting_exposure_panel"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/panel_end_margin"
        app:layout_constraintDimensionRatio="W,3:4"
        app:layout_constraintHeight_percent="0.7"
        app:layout_constraintVertical_bias="0.7"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="RtlHardcoded"
        tools:visibility="gone"
        />

    <dji.ux.panel.CameraSettingAdvancedPanel
        android:id="@+id/camera_setting_advenced_panel"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/panel_end_margin"
        app:layout_constraintDimensionRatio="W,3:4"
        app:layout_constraintHeight_percent="0.7"
        app:layout_constraintVertical_bias="0.7"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="RtlHardcoded"
        tools:visibility="gone"
        />

    <!-- ActiveTrackMissionControlUI -->
    <RelativeLayout
        android:id="@+id/tracking_bg_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintTop_toBottomOf="@+id/panel_top_bar"
        app:layout_constraintBottom_toTopOf="@+id/txt_status_message"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        >

        <Button
            android:id="@+id/confirm_btn"
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:layout_above="@id/tracking_stop_btn"
            android:text="A"
            android:visibility="invisible" />

        <ImageButton
            android:id="@+id/tracking_stop_btn"
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:layout_above="@id/reject_btn"
            android:background="#00000000"
            android:scaleType="fitXY"
            android:src="@drawable/mission_stop"
            android:text="Stop"
            android:visibility="invisible" />

        <Button
            android:id="@+id/reject_btn"
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:text="R"
            android:layout_marginBottom="110dp"
            android:layout_alignParentBottom="true"
            android:visibility="invisible" />

        <Button
            android:id="@+id/recommended_configuration_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_above="@+id/layout_pull_back"
            android:text="Config" />

        <LinearLayout
            android:id="@+id/layout_pull_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="8dp"
            android:layout_above="@id/layout_gesture_mode"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="後退"
                android:gravity="center"
                android:layout_gravity="center"
                android:textColor="@color/white" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/tracking_pull_back_tb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/layout_gesture_mode"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:layout_alignParentBottom="true"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="ジェスチャー"
                android:textColor="@color/white" />

            <androidx.appcompat.widget.SwitchCompat
                android:id="@+id/tracking_in_gesture_mode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center" />

        </LinearLayout>

    </RelativeLayout>

    <!-- TODO deprecatedなSlidingDrawerを使うのはあまり良くないが、サンプルなのでこれを利用する -->
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/panel_top_bar"
        app:layout_constraintBottom_toTopOf="@+id/txt_status_message"
        app:layout_constraintEnd_toEndOf="parent"
        >

        <!-- Active Track Information -->
        <SlidingDrawer
            android:id="@+id/tracking_drawer_sd"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:content="@+id/tracking_drawer_content_rl"
            android:handle="@+id/tracking_handle"
            >

            <ImageView
                android:id="@id/tracking_handle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="ContentDescription" />

            <RelativeLayout
                android:id="@id/tracking_drawer_content_rl"
                android:background="@color/black_transparent"
                android:padding="4dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <ScrollView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:clickable="false"
                    android:scrollbars="vertical">

                    <TextView
                        android:id="@+id/pointing_push_tv"
                        android:layout_width="250dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:scrollbars="vertical"
                        android:text="@string/push_info" />
                </ScrollView>
            </RelativeLayout>
        </SlidingDrawer>
    </RelativeLayout>

    <!-- カスタムここまで -->

    <dji.ux.beta.core.widget.remainingflighttime.RemainingFlightTimeWidget
        android:id="@+id/widget_remaining_flight_time"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/panel_top_bar" />
